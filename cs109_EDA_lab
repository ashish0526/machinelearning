        "  bestbook = subset[subset.rating == subset.rating.max()]\n",
        "  \n",
        "  \n",
        "  if bestbook.shape[0] > 1:\n",
        "    print(year,bestbook.name.values,bestbook.rating.values)\n",
        "  else:\n",
        "    print(year,bestbook.name.values[0],bestbook.rating.values[0])\n",
        "  "
